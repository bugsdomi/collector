// *************************************************************************
// Fenêtre Modale Login
// *************************************************************************
#idModalLogin.modal(tabindex='-1' role='dialog' aria-labelledby='idModalLoginTitle' aria-hidden='true')
  .modal-dialog.modal-dialog-centered(role='document')
    .modal-content
      #idModalLoginHeader.modal-header
        h5.modal-title Connexion
        button.close(type='button' data-dismiss='modal' aria-label='Fermer' aria-hidden='true') &times;
      .modal-body
        form#idLoginForm.form-Signin
          img.mb-4.mx-auto.d-block(src='static/images/favicon.png' alt="Logo du site 'Collect\'Or'" width='auto' height='72')
          h1.h3.mb-3.font-weight-normal Veuillez vous identifier
          label.sr-only(for='idLoginPseudo') Pseudonyme
          input#idLoginPseudo.form-control(type='text' name='loginPseudo' placeholder='Pseudonyme' required='' autofocus='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_&#$ \*\-]{2,19}$' title="Le 1er caractère doit être alphanumérique, les suivants également + ' ','_','&','#','-','*','$', et la longueur comprise entre 3 et 20 caractères")
          br
          label.sr-only(for='idLoginPassword') Mot de passe
          input#idLoginPassword.form-control(type='password' name='loginPassword' placeholder='Mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_&#$\*\-]{5,11}$' title="Le 1er caractère doit être alphanumérique, les suivants également + '_','&','#','-','*','$', et la longueur comprise entre 6 et 12 caractères")
          .row.justify-content-between
            #idSignIn.mt-1.mb-2.col-6.xs-col-6.text-left.speedKey
              | Cr&eacute;er un compte
            #idLostPWD.mt-1.mb-2.col-6.xs-col-6.text-right.speedKey
              | Mot de passe oubli&eacute; ?
          p#idLoginAlertMsg.lead.mt-5.display-4.text-danger.text-center.alertMsg Pseudo et/ou mot de passe inconnus
          .modal-footer
            button#idConnectionBtn.btn.btn-lg.btn-success.btn-block(type='submit') Connexion
// *************************************************************************
// Fenêtre Modale Inscription
// *************************************************************************
#idModalSignIn.modal(tabindex='-1' role='dialog' aria-labelledby='idModalSignInTitle' aria-hidden='true')
  .modal-dialog.modal-dialog-centered(role='document')
    .modal-content
      #idModalSignInHeader.modal-header
        h5.modal-title Cr&eacute;ation de compte
        button.close(type='button' data-dismiss='modal' aria-label='Fermer' aria-hidden='true') &times;
      .modal-body
        form#idSignInForm.form-Signin
          img.mb-4.mx-auto.d-block(src='static/images/favicon.png' alt="Logo du site 'Collect\'Or'" width='auto' height='72')
          h1.h3.mb-3.font-weight-normal Veuillez saisir vos informations
          .mb-3
            label.sr-only(for='idSignInEmail')
              | Email 
              span.text-muted
            .input-group
              .input-group-prepend
                span.input-group-text @
              input#idSignInEmail.form-control(type='email' name='signInEmail' placeholder='exemple@provider.com' required='' autofocus='')
          fieldset.border.rounded.p-3
            label.sr-only(for='idSignInPseudo') Pseudonyme
            input#idSignInPseudo.form-control(type='text' name='signInPseudo' placeholder='Pseudonyme' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_&#$ \*\-]{2,19}$' title="Le 1er caractère doit être alphanumérique, les suivants également + ' ','_','&','#','-','*','$', et la longueur comprise entre 3 et 20 caractères")
            br
            label.sr-only(for='iSignInPassword') Mot de passe
            input#idSignInPassword.form-control(type='password' name='signInPassword' placeholder='Mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_&#$\*\-]{5,11}$' title="Le 1er caractère doit être alphanumérique, les suivants également + '_','&','#','-','*','$', et la longueur comprise entre 6 et 12 caractères")
            input#idSignInConfirmPassword.form-control(type='password' name='signInConfirmPassword' placeholder='Confirmez le mot de passe' required='' pattern='^[a-zA-Z0-9]{1}[a-zA-Z0-9_&#$\*\-]{5,11}$' title="Le 1er caractère doit être alphanumérique, les suivants également + '_','&','#','-','*','$', et la longueur comprise entre 6 et 12 caractères")
            label.sr-only(for='idSignInConfirmPassword') Confirmez le mot de passe
          p#idSignInAlertMsg.lead.mt-5.display-4.text-danger.text-center.alertMsg L&apos;e-mail et/ou le pseudo sont d&eacute;j&agrave; attribu&eacute;s
          .modal-footer
            button#idSignInBtn.btn.btn-lg.btn-success.btn-block(type='submit') Cr&eacute;ation
